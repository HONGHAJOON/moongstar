<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.dog">
	<select id="selectDog" parameterType="String"
		resultType="dto.Dog">
		<![CDATA[
			select * from dog where memId=#{memId}
		]]>
	</select>

	<insert id="insertDog" parameterType="dto.Dog">
		<![CDATA[
			insert into dog (dogName,dogNum,memId,dogAge,dogProfile,dogGender)
			values (#{dogName},#{dogNum},#{memId},#{dogAge},#{dogProfile},#{dogGender})
		]]>
	</insert>
	<update id="updateDog">
		UPDATE dog SET dogName=#{dogName} dogAge=#{dogAge} dogProfile=#{dogProfile} dogGender=#{dogGender}
		WHERE memId=#{memId}
	</update>
	<delete id="deleteDog">
		DELETE FROM dog WHERE memId=#{memId}
	</delete>
</mapper>