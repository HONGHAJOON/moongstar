<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.member">
	<select id="selectMember" parameterType="String" resultType="dto.Member">
		<![CDATA[
			select * from member where memId=#{memId}
		]]>
	</select>
	
	<insert id="insertMember" parameterType="dto.Member">
		<![CDATA[
			insert into member (memId,memPw,memNick,memPhone,memAddress1,memAddress2,memEmail,memProfile)
			values (#{memId},#{memPw},#{memNick},#{memPhone},#{memAddress1},#{memAddress2},#{memEmail},#{memProfile})
		]]>
	</insert>
</mapper>